package di.basic2;

public class DriverMain {
	public static void main(String[] args) {
		
		
		Tire hankook = new HankookTire();
		Tire kumho = new kumhoTire();
		
		
		Car car = new Car();
		//이제 이 자동차에게 의존주입을 할 수 있다! ->  setter를 우선 써보자! 
		
		//car.setTire(kumho);
		//협상이 안되어서  hankook으로 바꾸려면? 개발자는 한국 타이어로만 바꾸면 된다! 
		car.setTire(hankook);
		
		
		car.printTireBrand();
		
		

	}
}